{
    "version": 2,
    "builds": [
      {
        "src": "api/extract.js",
        "use": "@vercel/node@2.0.0"
      },
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build@2.0.0"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  