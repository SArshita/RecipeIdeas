{
    "version": 3,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
      "api/*.js": {
        "runtime": "nodejs20.x"
      }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  