{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node@2.0.0"
        },
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build@2.0.0"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "framework": "vite",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "env": {
        "NODE_ENV": "production",
        "OPENROUTER_API_KEY": "@openrouter_api_key"
    },
    "github": {
        "enabled": true,
        "silent": false
    }
}